  procedures :
   -
     name : ChangeModeAgenda
     procedure_id : 2011681686955531650
     type_code : 14
     code : |1-
      // Résumé : Change le mode de l'agenda
      // Syntaxe :
      //ChangeModeAgenda (<eMode> est FEN_Accueil.EAgendaMode)
      //
      // Paramètres :
      //	eMode (FEN_Accueil.EAgendaMode) : Le mode à afficher
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ChangeModeAgenda(eMode est un EAgendaMode) <ihm>
      
      BTN_Jour..Enfoncé = faux
      BTN_Semaine..Enfoncé = faux
      BTN_Mois..Enfoncé = faux
      
      geModeAgenda = eMode
      
      selon eMode
      	CAS AgendaMode_Jour
      		BTN_Jour..Enfoncé = vrai
      		AGD_RDV.ChangeMode(agzJour)
      	CAS AgendaMode_Semaine_5
      		BTN_Semaine..Enfoncé = vrai
      		AGD_RDV.ChangeMode(agzSemaineSur5)
      	CAS AgendaMode_Mois
      		BTN_Mois..Enfoncé = vrai
      		AGD_RDV.ChangeMode(agzMois)
      		
      FIN
      
      RafraichitInfoAgenda()
     type : 458752
   -
     name : RafraichitAgenda
     procedure_id : 2011681686955597530
     type_code : 14
     code : |1-
      // Résumé : Rafraîchit l'agenda
      //
      PROCEDURE RafraichitAgenda() 
      
      AGD_RDV.SupprimeTout()
      
      pour TOUT rdv 
      	monrdv est un RendezVous
      	
      	monrdv.Titre = rdv.Titre
      	monrdv.Contenu = rdv.Contenu
      	monrdv.DateDébut = rdv.DateDébut
      	monrdv.DateFin = rdv.DateFin
      	
      	monrdv.ID = rdv.IDRDV
      	
      	// Recherche de la couleur
      	HLitRecherche(fondrdv,IDFondRDV,rdv.IDFondRDV)
      	monrdv.CouleurFond	= HTMLVersRVB(fondrdv.CouleurFond)
      	
      	AGD_RDV.AjouteRendezVous(monrdv)
      FIN
     type : 458752
   -
     name : RafraichitInfoAgenda
     internal_properties : CAAAAAgAAABE/MVF5aMcL76pBEaXWlOMo74FA7G+zB3cXMB0Xw8mH65mtt7N/lxw9mTVQhIPIwmg8Y+KRdn5XJOmcuSc3mL0B7Oo9URtGWVkU7W5N1/3z8B77Ni077BPL5wX36RNiezmzeaWe65eGWmUn9Cxc7AZhOJoMe9gJzwxrn31ctq2rwrkFrYH9Fm0og+Rh1omPzvI3fC9ZHguVAWkuigjjigLYdoCkoIf
     procedure_id : 2011681755675155896
     type_code : 14
     code : |1-
      // Résumé : Rafraîchit les infos d'agenda
      //
      PROCEDURE RafraichitInfoAgenda() <IHM>
      
      dDebut est une date = AgendaPosition(AGD_RDV)
      
      
      
      selon geModeAgenda
      	CAS AgendaMode_Jour
      		MaFenêtre..titre = "Champ Agenda"
      	CAS AgendaMode_Semaine_5,AgendaMode_Semaine_7
      		MaFenêtre..Titre = "Semaine " + NuméroDeSemaine(dDebut)		
      	CAS AgendaMode_Mois
      		<COMPILE SI TypeConfiguration=Android>
      			MaFenêtre..Titre = DateVersChaîne(AgendaPosition(AGD_RDV,agMoisEnCours),"Mmmm")
      		<sinon>
      			MaFenêtre..Titre = "Champ Agenda"
      		<FIN>
      FIN
     type : 458752
