   -
     code : |1+
      PROCEDURE MaFenêtre()
      
      // Modes de l'agenda
      EAgendaMode est une énumération
      	AgendaMode_Jour
      	AgendaMode_Semaine_5
      	AgendaMode_Semaine_7
      	AgendaMode_Mois
      FIN
      
      // Mode par défaut
      geModeAgenda est un EAgendaMode
      gRDVSélectionné est un RendezVous
      
      // Si l'application est sur un grand écran (tablette ou PC)
      SI TypeEcran() DANS (teTablette, teOrdinateur) ALORS
      	// Tablette
      	geModeAgenda = AgendaMode_Semaine_5
      SINON
      	// Téléphone
      	geModeAgenda = AgendaMode_Jour
      FIN
      
      
      // Changement du mode
      ChangeModeAgenda(geModeAgenda)
   -
     code : |1+
      
      // Initialisation de la base de données
      InitDB()
      
      // Remplissage de l'agenda
      RafraichitAgenda()
